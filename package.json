{
  "name": "tunedropper-scraper",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=13.9.0"
  },
  "type": "module",
  "scripts": {
    "start": "node ./src/server",
    "dev": "nodemon --inspect ./src/server/index.js",
    "tunnel": "~/Downloads/ngrok http 8080"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.7",
    "localtunnel": "^2.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn run prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "btoa": "^1.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "superagent": "^5.2.2"
  }
}
